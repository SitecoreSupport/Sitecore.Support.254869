<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <pipelines>
      <getChromeData>
        <processor patch:instead="*[@type='Sitecore.XA.Feature.StickyNotes.Pipelines.GetChromeData.AddStickyNotesButton, Sitecore.XA.Feature.StickyNotes']" type="Sitecore.Support.XA.Feature.StickyNotes.Pipelines.GetChromeData.AddStickyNotesButton, Sitecore.Support.254869" />
      </getChromeData>
    </pipelines>
    <services>
      <!-- Sitecore.XA.Foundation.Editing.config has to be overwritten instead of config patched to ensure no Unicode char in config -->
    </services>
  </sitecore>
</configuration>